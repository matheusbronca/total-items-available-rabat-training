{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "getPokemonByName": {
      "path": "/_v/pokemon/:pokemonName",
      "public": true
    },
    "getPokemonsRange": {
      "path": "/_v/pokemons-range/:qty",
      "public": true
    },
    "getAvailableItems": {
      "path": "/_v/total-items-available/:skuId",
      "public": true,
      "access": "authorized",
      "policies": [
        {
          "effect": "allow",
          "actions": [
            "get"
          ],
          "principals": [
            "vrn:apps:*:*:*:*",
            "vrn:proxy:*:*:*:*"
          ]
        }
      ]
    }
  }
}
