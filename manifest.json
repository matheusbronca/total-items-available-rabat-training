{
  "name": "total-items-available",
  "vendor": "frncubo",
  "version": "1.0.3",
  "title": "Total Items Available",
  "description": "An App who get the total available items in the inventory of a selected SKU",
  "categories": [],
  "dependencies": {
    "vtex.product-context": "0.x",
    "vtex.catalog-api-proxy": "0.x"
  },
  "builders": {
    "node": "6.x",
    "docs": "0.x",
    "react": "3.x",
    "store": "0.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/logistics/pvt/inventory/skus/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "/api/logistics/pvt/inventory/skus/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.com.br",
        "path": "/api/logistics/pvt/inventory/skus/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/logistics/pvt/inventory/skus/"
      }
    },
    {
      "name": "LogisticsAdmin"
    }
  ],
  "billingOptions": {
    "type": "free",
    "availableCountries": [
      "*"
    ],
    "support": {
      "url": "https://frncomunicacao.com.br",
      "email": "matheus.bronca@frncubo.com",
      "phone": "+5562984546866"
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
